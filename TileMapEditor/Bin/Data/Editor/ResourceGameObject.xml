<?xml version="1.0" encoding="UTF-8"?><resoucegameobject><!--	iconres是依赖的美术ResourceTile名称；	unitTileW和unitTileH用来检查是否和地图tile的尺寸是否匹配；	iconid是子资源名；	对象定位在(xOffset,yOffset)，图像左上角离定位点的偏差，定位点所在的格子是(0,0)；	重心坐标是所有阻挡点均值，float类型；	如果obstacle为空，对象不影响阻挡信息。-->	<group name="npc" iconres="tile2" mapType="1" unitTileW="100" unitTileH="50">		<item name="npc01" iconid="000" xOffset="25" yOffset="16" xBarycentric="0" yBarycentric="0" >			<obstacle x="0" y="0"/>			<obstacle x="0" y="1"/>			<obstacle x="0" y="-1"/>		</item>		<item name="npc02" iconid="001" />		<item name="npc03" iconid="002" />	</group>	<group name="monster" iconres="tile2" mapType="1" unitTileW="100" unitTileH="50">		<item name="monster01" iconid="005" />		<item name="monster02" iconid="006" />		<item name="monster03" iconid="007" />	</group></resoucegameobject>