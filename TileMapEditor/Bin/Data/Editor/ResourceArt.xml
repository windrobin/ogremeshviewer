<?xml version="1.0" encoding="UTF-8"?><resourceart><!--	iconsize是资源缩略图中图标大小；	tilew和tileh是大图中小块的大小；	一个目录中文件类型要一致，大小可以不同，文件名后缀不需要加'.'；-->	<background>		<item name="bg1" res="art/background/bg1.png" iconsize="128" />	</background>	<tiles>		<!-- tiles in one big image-->		<item type="image" name="tile1" file="art/tiles/tile1.jpg" iconsize="64" tilew="60" tileh="60" count="150"/>		<item type="image" name="tile3" file="art/tiles/tile3.jpg" iconsize="64" tilew="256" tileh="256" count="1"/>		<!-- tiles in subdir, named with ext-->		<item type="folder" name="tile2" folder="art/tiles/tiles2/" iconsize="64" ext="png" />		<item type="folder" name="ground" folder="art/aok/Ground/" iconsize="64" ext="png" />		<item type="folder" name="castle" folder="art/aok/Castle/" iconsize="64" ext="png" />	</tiles></resourceart>